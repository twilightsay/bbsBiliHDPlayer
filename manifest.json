{
  "manifest_version": 3,
  "name": "论坛BiliHD播放器",
  "version": "1.1",
  "permissions": [
  "cookies",
  "webRequest",
  "tabs"
],
"host_permissions": [
  "*://*.bilibili.com/*",
  "https://www.bilibili.com/*",
  "https://.bilibili.com/"
],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}